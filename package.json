{
  "name": "micro-swagger",
  "version": "0.1.11",
  "description": "Import swagger files from aws api gateway, and create a swagger ui for them.",
  "main": "build/index.js",
  "bin": {
    "micro-swagger": "micro-swagger.js"
  },
  "scripts": {
    "start": "nodemon --exec babel-node ./src/index.js",
    "build": "babel src --out-dir ./build",
    "deploy": "npm run build && npm version patch && npm publish"
  },
  "keywords": [
    "swagger",
    "aws",
    "lambda"
  ],
  "author": "Nir Adler",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "apigateway-export-tool": "^0.0.13",
    "commander": "^5.0.0",
    "configstore": "^5.0.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "ejs": "^3.0.0",
    "express": "^4.17.1",
    "fs-extra": "^9.0.0",
    "morgan": "^1.9.1",
    "open": "^7.0.0",
    "xdg-basedir": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.6",
    "@babel/node": "7.8.7",
    "@babel/preset-env": "7.9.6",
    "nodemon": "2.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/niradler/micro-swagger.git"
  },
  "bugs": {
    "url": "https://github.com/niradler/micro-swagger/issues"
  },
  "homepage": "https://github.com/niradler/micro-swagger#readme"
}
