{
  "name": "micro-swagger",
  "version": "0.0.9",
  "description": "Import swagger files from aws api gateway, and create a swagger ui for them.",
  "main": "index.js",
  "bin": {
    "micro-swagger": "micro-swagger.js"
  },
  "scripts": {
    "start": "nodemon --exec babel-node ./index.js",
    "publish": "mkdir build && npm run build && npm publish && rm -r build",
    "build": "babel src --out-dir build"
  },
  "keywords": [
    "swagger",
    "aws",
    "lambda"
  ],
  "author": "Nir Adler",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "apigateway-export-tool": "0.0.9",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "nodemon": "^1.18.11"
  }
}
